{
  "manifest_version": 2,
  "name": "My React Extension",
  "version": "1.0",
  "description": "A basic React app as Chrome extension",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon.ico",
      "48": "logo192.png",
      "128": "logo512.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  }
}